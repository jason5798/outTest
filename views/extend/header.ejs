<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title><%= title%></title>
	<%- include include_style.ejs %>

</head>

<body>
	<div id="wrapper">
        <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <a class="navbar-brand" href="/tools">Device admin</a>

            </div>

        </nav>
           <!-- /. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
          <div class="sidebar-collapse">

              <ul class="nav" id="main-menu">


                    <li  >
                        <% if(title ==='Index'){ %>
                          <a class="active-menu"  href="/">
                        <% }else{ %>
                          <a href="/">
                        <% } %>
                          <i class="fa fa-table fa-3x"></i>
                          <label class="fa fa-2x" style="color:#ffffff">Device</label>
                        </a>
                    </li>
                    <li  >
                        <% if(title ==='Setting'){ %>
                          <a class="active-menu"  href="/setting">
                        <% }else{ %>
                          <a href="/setting">
                        <% } %>
                          <i class="fa fa-pencil-square-o fa-3x"></i>
                          <label class="fa fa-2x" style="color:#ffffff">Setting</label>
                        </a>
                    </li>

              </ul>
          </div>
        </nav>
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper" >
          <div id="page-inner">
             <div class="row">
             <% if(title ==='Index'){ %>
                <div >
                  <div class="col-md-6">
                      <div class="col-md-12" style="margin-top: 0px;">
                        <label>Start Date</label>
                        <input type="text" id="date" name="date" size="10" value="" readonly>
                        <input type="button" value="..." id="BTN" name="BTN">
                    </div>
                  </div>
                  <!-- Select Range -->
                  <div class="col-md-3">
                    <div class="form-group">
                      <label>Select Range</label>
                      <select "selected_time" id="time_option" name="time_option">
                        <% if(select.option == 0){ %>
                          <option value="0" selected>&nbsp&nbspOne Day&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% }else{ %>
                          <option value="0">&nbsp&nbspOne Day&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% } %>

                        <% if(select.option == 1){ %>
                          <option value="1" selected>&nbsp&nbspOne Week&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% }else{ %>
                          <option value="1">&nbsp&nbspOne Week&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% } %>

                        <% if(select.option == 2){ %>
                          <option value="2" selected>&nbsp&nbspOne Month&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% }else{ %>
                          <option value="2">&nbsp&nbspOne Month&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% } %>

                        <% if(select.option == 3){ %>
                          <option value="3" selected>&nbsp&nbspThree Month&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% }else{ %>
                          <option value="3">&nbsp&nbspThree Month&nbsp&nbsp&nbsp&nbsp&nbsp</option>
                        <% } %>

                      </select>
                    </div> <!-- form-group -->
                  </div> <!-- col-md-3 -->

            <% } %>
            <div class="col-md-12 column" style="margin-top: 10px;">